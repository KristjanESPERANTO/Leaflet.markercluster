{
  "name": "leaflet.markercluster",
  "repository": {
    "type": "git",
    "url": "https://github.com/Leaflet/Leaflet.markercluster"
  },
  "version": "1.5.5",
  "description": "Provides Beautiful Animated Marker Clustering functionality for Leaflet",
  "bugs": {
    "url": "https://github.com/Leaflet/Leaflet.markercluster/issues"
  },
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "happen": "^0.3.2",
    "leaflet": "^2.0.0-alpha.1",
    "mocha": "^11.7.4",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "rollup": "4.52.5",
    "rollup-plugin-git-version": "0.3.1",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-prettier": "^5.0.3",
    "uglify-js": "3.19.3"
  },
  "peerDependencies": {
    "leaflet": ">=2.0.0-alpha.1"
  },
  "main": "dist/leaflet.markercluster-src.js",
  "style": "dist/MarkerCluster.css",
  "scripts": {
    "test": "npm run lint:code && npm run rollup && node spec/run-tests.mjs",
    "build": "npm run lint:code && npm run rollup && npm run uglify",
    "prepublish": "npm run build",
    "lint": "npm run lint:code && npm run lint:style && npm run lint:format",
    "lint:code": "eslint",
    "lint:style": "stylelint **/*.css",
    "lint:format": "prettier --check .",
    "lint:fix": "eslint --fix && stylelint --fix **/*.css && prettier --write .",
    "release": "npx bumpp --no-push",
    "rollup": "rollup -c build/rollup-config.mjs",
    "uglify": "uglifyjs dist/leaflet.markercluster-umd.js -c -m -o dist/leaflet.markercluster.js --source-map 'filename=dist/leaflet.markercluster.js.map,content=dist/leaflet.markercluster-umd.js.map,url=leaflet.markercluster.js.map'"
  },
  "keywords": [
    "gis",
    "map",
    "cluster"
  ],
  "license": "MIT"
}
