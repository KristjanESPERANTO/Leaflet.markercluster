{
  "name": "@kristjan.esperanto/leaflet.markercluster",
  "version": "3.0.0-alpha.1",
  "description": "Provides beautiful animated Marker clustering functionality for Leaflet.",
  "homepage": "https://kristjanesperanto.github.io/Leaflet.markercluster/",
  "repository": {
    "type": "git",
    "url": "https://github.com/KristjanESPERANTO/Leaflet.markercluster"
  },
  "bugs": {
    "url": "https://github.com/KristjanESPERANTO/Leaflet.markercluster/issues"
  },
  "author": "Dave Leaver",
  "license": "MIT",
  "type": "module",
  "main": "dist/leaflet.markercluster.js",
  "module": "dist/leaflet.markercluster.js",
  "style": "dist/MarkerCluster.css",
  "sideEffects": false,
  "publishConfig": {
    "tag": "next",
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/leaflet.markercluster.js",
      "default": "./dist/leaflet.markercluster.js"
    },
    "./dist/MarkerCluster.css": "./dist/MarkerCluster.css",
    "./dist/MarkerCluster.Default.css": "./dist/MarkerCluster.Default.css"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md",
    "LICENCE.md"
  ],
  "keywords": [
    "gis",
    "map",
    "cluster",
    "leaflet",
    "markers"
  ],
  "peerDependencies": {
    "leaflet": ">=2.0.0-alpha.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@eslint/markdown": "^7.5.1",
    "@rollup/plugin-json": "^6.1.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "chai": "^6.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsdoc": "^61.2.1",
    "globals": "^16.5.0",
    "leaflet": "^2.0.0-alpha.1",
    "mocha": "^11.7.5",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "rollup": "4.53.2",
    "sinon": "^21.0.0",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-prettier": "^5.0.3"
  },
  "scripts": {
    "test": "npm run lint:code && npm run build && node spec/run-tests.js",
    "build": "npm run lint:fix && rollup -c build/rollup-config.js",
    "prepublishOnly": "npm run build",
    "lint": "npm run lint:code && npm run lint:style && npm run lint:format",
    "lint:code": "eslint",
    "lint:style": "stylelint **/*.css",
    "lint:format": "prettier --check .",
    "lint:fix": "eslint --fix && stylelint --fix **/*.css && prettier --write ."
  }
}
